<%- include("header"); -%>
    <h1> <%= day %> </h1>
    <ul>
        <% items.forEach((val)=>{ %>
            <input type="checkbox" name="" id="">
            <div> <%= val  %> </div>
       <% }) %>
    </ul>
    <form action="/" method="POST">
        <input type="text" name="task" autocomplete="off" required>
        <button type="submit" name="button" value=<%= day %> >+</button>
    </form>
 <%- include("footer"); -%>